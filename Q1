#include <iostream>
#include <stdbool.h>
using namespace std;
class Stack
{

    int arr[5];
    int idx;

public:
    Stack()
    {
        idx = -1;
    }
    void push(int value)
    {
        if (idx == 4)
        {
            cout << "stack overflow";
            return;
        }
        idx++;
        arr[idx] = value;
    }
    void pop()
    {
        if (idx == -1)
        {
            cout << "stack underflow";
            return;
        }
        else
        {
            idx--;
        }
    }
    bool isempty()
    {
        if (idx == -1)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    bool isfull()
    {
        if (idx == 4)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    void display()
    {
        if (idx == -1)
        {
            cout << "stack is empty";
        }

        for (int i = 0; i <= idx; i++)
        {
            cout << arr[i] << endl;
        }
    }
    void peek()
    {
        if (idx == -1)
        {
            cout << "stack is empty";
        }
        else
        {
            cout << arr[idx] << endl;
        }
    }
};
int main()
{
    Stack st;
    st.push(9);
    st.push(10);
    st.push(100);
    st.push(10000);
    st.push(100000);
    // st.display();
    st.pop();
    // st.display();
    st.peek();
}
