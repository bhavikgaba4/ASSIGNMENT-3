#include <iostream>
#include<stack>
using namespace std;

int main() {
    string s = "DATA STRUCTURES";
    stack<char>st;
    int x = s.length()-1;
    for(int i = 0;i<=x;i++){
        st.push(s[i]);
    }
    string t="";
    while(st.size()>0){
        t.push_back(st.top());
        st.pop();
    }
    for(int i = 0;i<=x;i++){
        cout<<t[i];
    }

}
